{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"properties": {
		"number": {
			"type": "integer"
		},
		"name": {
			"type": "string",
			"minLength": 1
		},
		"syllabus": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"parts": {
			"type": "array",
			"items": {
				"type": "string"
			}
		}
	},
	"required": ["number", "name"],
	"additionalProperties": false
}
